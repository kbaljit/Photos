            <Button fx:id="Search" mnemonicParsing="false" text="Search" visible="false" />
            <Button fx:id="Caption" mnemonicParsing="false" text="Caption" visible="false" />
            <Button fx:id="Tag" mnemonicParsing="false" text="Tag" visible="false" />
            <Button fx:id="Copy" mnemonicParsing="false" text="Copy" visible="false" />
            <Button fx:id="Move" mnemonicParsing="false" text="Move" visible="false" />
            <Button fx:id="SlideShow" mnemonicParsing="false" text="SlideShow" visible="false" />
        </buttons>
      </ButtonBar>
